{
    "ver": "0.0.1",
    "authors": [
        "dakotablair"
    ],
    "contact": "",
    "categories": ["active"],
    "widgets": {
        "input": null,
        "output": null
    },
    "parameters": [
        {
            "id": "reads_ref",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseFile.SingleEndLibrary" ]
            }
        },
        {
            "id" : "output_name",
            "optional" : false,
            "advanced" : false,
            "allow_multiple" : false,
            "default_values" : [ "ReadsHead" ],
            "field_type" : "text",
            "text_options" : {
              "valid_ws_types" : [ "KBaseFile.SingleEndLibrary" ],
              "is_output_name":true
             }
        },
        {
            "id": "bam_file",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "field_type": "dynamic_dropdown",
            "dynamic_dropdown_options": {
                "data_source": "ftp_staging"
            }
        },
        {

            "id": "sequencing_tech",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "field_type": "dynamic_dropdown",
            "dynamic_dropdown_options": {
                "options": [{
                    "value": "illumina",
                    "display": "Illumina"
                }, {
                    "value": "pacbio clr",
                    "display": "PacBio CLR"
                }, {
                    "value": "pacbio ccs",
                    "display": "PacBio CCS"
                }, {
                    "value": "iontorrent",
                    "display": "IonTorrent"
                }, {
                    "value": "nanopore",
                    "display": "NanoPore"
                }, {
                    "value": "unknown",
                    "display": "Unknown"
                }]
            }
        },
        {
            "id": "single_genome",
            "optional": false,
            "advanced": false,
            "field_type": "checkbox",
            "default_values": [
                "0"
            ],
            "checkbox_options": {
                "unchecked_value": 0,
                "checked_value": 1
            }

        },
        {
          "id": "interleaved",
          "optional": false,
          "advanced": false,
          "allow_multiple": false,
          "default_values": [0],
          "field_type": "checkbox",
          "checkbox_options": {
            "checked_value": 1,
            "unchecked_value": 0
          }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "kb_bedtools",
            "method": "run_kb_bedtools",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },{
                    "narrative_system_variable": "workspace_id",
                    "target_property": "workspace_id"
                },{
                    "input_parameter": "reads_ref",
                    "target_property": "reads_ref",
                    "target_type_transform": "resolved-ref"
                },{
                    "input_parameter": "output_name",
                    "target_property": "output_name"
                }
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },{
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                }, {
                    "service_method_output_path": [0, "obj_ref"],
                    "target_property": "obj_ref"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
